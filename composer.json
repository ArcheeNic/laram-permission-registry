{
    "name": "artprog/permission-registry",
    "description": "Laravel module for managing user permissions registry",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Arthur",
            "email": "example@example.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/database": "^11.0",
        "illuminate/support": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "Artprog\\PermissionRegistry\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Artprog\\PermissionRegistry\\ServiceProvider"
            ],
            "aliases": {
                "PermissionRegistry": "Artprog\\PermissionRegistry\\Facades\\PermissionRegistry"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
